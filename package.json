{
  "name": "redux-react-webpack-boilerplate",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack/development.js",
    "test": "NODE_ENV=test karma start config/test/karma.js --single-run",
    "test:watch": "yarn run test -- --auto-watch --no-single-run",
    "build:production": "webpack --config config/webpack/production.js --color -p --progress",
    "validate": "run-p validate:styles validate:js",
    "validate:styles": "stylelint 'app/**/*.css'",
    "validate:js": "eslint 'app/**/*.js'",
    "validate:webpack:dev": "webpack-validator config/webpack/development.js --env.dev"
  },
  "repository": {
    "url": "git+https://github.com/emartini/redux-react-webpack-boilerplate.git",
    "type": "git"
  },
  "keywords": [
    "react",
    "webpack",
    "redux",
    "react-hot-loader"
  ],
  "author": "Esteban Martini",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/emartini/redux-react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/emartini/redux-react-webpack-boilerplate#readme",
  "dependencies": {
    "es6-promise": "^4.0.5",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.0",
    "cheerio": "^0.22.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "minimist": "^1.2.0",
    "mocha": "^3.1.2",
    "normalize.css": "^5.0.0",
    "npm-run-all": "^3.1.1",
    "null-loader": "^0.1.1",
    "postcss": "^5.2.5",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^1.1.0",
    "postcss-modules-values": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^14.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.27",
    "webpack-cleanup-plugin": "^0.4.1",
    "webpack-dev-server": "^2.1.0-beta.11",
    "webpack-validator": "^2.2.9"
  },
  "postcss": {
    "plugins": {
      "postcss-modules-values": {},
      "postcss-nested": {},
      "postcss-cssnext": {
        "browsers": [
          "Explorer > 9",
          "Android >= 4.0",
          "Firefox > 41",
          "Safari > 7",
          "iOS >= 7",
          "last 3 versions"
        ],
        "features": {
          "applyRule": false
        }
      }
    }
  }
}
